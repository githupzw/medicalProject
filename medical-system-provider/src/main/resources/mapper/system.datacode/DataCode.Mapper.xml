<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zwsyd.medical.system.datacode.dao.DataCodeDao">
	<select id="findListByQuery" parameterType="DataCode" resultMap="resultMap">
		select
			dc.id,
			dcc.id categoryId,
			dcc.name categoryName,
			dcc.code categoryCode,
			dc.name,
			dc.code,
			dc.status,
			dc.createUser,
			dc.createTime,
			dc.updateUser,
			dc.updateTime
		from
			sys_data_code dc join sys_data_code_category dcc on dc.dataCategory=dcc.id
		<where>
			<trim suffixOverrides="and">
				<if test="id != null and id !=''">dc.id=#{id} and </if>
				<if test="name != null and name !=''">dc.name like concat('%',#{name},'%') and </if>
				<if test="code != null and code !=''">dc.code=#{code} and </if>
				<if test="status != null and status !=''">dc.status=#{status} and </if>
				<if test="dataCategory != null and dataCategory.id != null and dataCategory.id !=''">dcc.id=#{dataCategory.id} and </if>
				<if test="dataCategory != null and dataCategory.name != null and dataCategory.name !=''">dcc.name like concat('%',#{dataCategory.name},'%') and </if>
				<if test="dataCategory != null and dataCategory.code != null and dataCategory.code !=''">dcc.code=#{dataCategory.code} and </if>
			</trim>
		</where>
	</select>

	<insert id="save" parameterType="DataCode">
		insert into sys_data_code_category(
		<trim suffixOverrides=",">
			<if test="id != null and id != ''">id,</if>
			<if test="dataCategory != null and dataCategory.id !=null and dataCategory.id != ''">dataCategory,</if>
			<if test="name != null and name != ''">name,</if>
			<if test="code != null and code != ''">code,</if>
			<if test="status != null and status != ''">status,</if>
			<if test="createUser != null and createUser != ''">createUser,</if>
			<if test="createTime != null and createTime != ''">createTime,</if>
			<if test="updateUser != null and updateUser != ''">updateUser,</if>
			<if test="updateTime != null and updateTime != ''">updateTime,</if>
		</trim>
		) values (
		<trim suffixOverrides=",">
			<if test="id != null and id != ''">#{id},</if>
			<if test="dataCategory != null and dataCategory.id !=null and dataCategory.id != ''">#{dataCategory.id},</if>
			<if test="name != null and name != ''">#{name},</if>
			<if test="code != null and code != ''">#{code},</if>
			<if test="status != null and status != ''">#{status},</if>
			<if test="createUser != null and createUser != ''">#{createUser},</if>
			<if test="createTime != null and createTime != ''">#{createTime},</if>
			<if test="updateUser != null and updateUser != ''">#{updateUser},</if>
			<if test="updateTime != null and updateTime != ''">#{updateTime},</if>
		</trim>
		)
	</insert>

	<update id="update" parameterType="DataCode">
		update sys_data_code_category
		<set>
			<trim suffixOverrides=",">
				<if test="dataCategory != null and dataCategory.id !=null and dataCategory.id != ''">dataCategory=#{dataCategory.id},</if>
				<if test="name != null and name != ''">name=#{name},</if>
				<if test="code != null and code != ''">code=#{code},</if>
				<if test="status != null and status != ''">status=#{status},</if>
				<if test="createUser != null and createUser != ''">createUser=#{createUser},</if>
				<if test="createTime != null and createTime != ''">createTime=#{createTime},</if>
				<if test="updateUser != null and updateUser != ''">updateUser=#{updateUser},</if>
				<if test="updateTime != null and updateTime != ''">updateTime=#{updateTime},</if>
			</trim>
		</set>
		<where>
			<trim suffixOverrides="and">
				<if test="id != null and id != ''">id=#{id} and </if>
			</trim>
		</where>
	</update>

	<resultMap id="resultMap" type="DataCode">
		<id property="id" javaType="java.lang.String" column="id"/>
		<result property="name" javaType="java.lang.String" column="name" />
		<result property="code" javaType="java.lang.String" column="code" />
		<result property="status" javaType="java.lang.String" column="status" />
		<result property="createUser" javaType="java.lang.String" column="createUser" />
		<result property="createTime" javaType="java.util.Date" column="nacreateTimeme" />
		<result property="updateUser" javaType="java.lang.String" column="updateUser" />
		<result property="updateTime" javaType="java.util.Date" column="updateTime" />
		<association property="dataCategory" javaType="DataCodeCategory">
			<id property="id" javaType="java.lang.String" column="categoryId"/>
			<result property="name" javaType="java.lang.String" column="categoryName" />
			<result property="code" javaType="java.lang.String" column="categoryCode" />
		</association>
	</resultMap>
</mapper>